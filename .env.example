# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token
DISCORD_CLIENT_ID=your_client_id
DISCORD_GUILD_ID=your_guild_id_for_development

# n8n Webhooks (for Supabase integration)
N8N_WEBHOOK_BASE_URL=http://your-n8n-instance:5678/webhook
N8N_SNIPPET_WEBHOOK=discord-snippet
N8N_COMPONENT_WEBHOOK=component-browser
N8N_BACKGROUND_WEBHOOK=background-search

# Supabase (Optional - for direct queries)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Cloudinary (for image processing)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# StackBlitz (for component sandbox)
STACKBLITZ_BASE_URL=https://stackblitz.com/edit

# Bot Configuration
BOT_PREFIX=!
LOG_LEVEL=info
NODE_ENV=development
